	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\lib\\avcmd.js",
			"sourceMaps": true,
			"outFiles": [ "${workspaceRoot}/lib/**/*.js",  "${workspaceRoot}/node_modules/**/*.js" ],
			"args": [ "list" ]
		}
	]
}